{
	"name": "GithubGet",
	"properties": {
		"activities": [
			{
				"name": "APIGet",
				"type": "ExecutePipeline",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "APIGet",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"BaseUrl": {
							"value": "@pipeline().parameters.BaseUrl",
							"type": "Expression"
						},
						"RelativeUrl": {
							"value": "@pipeline().parameters.RelativeUrl",
							"type": "Expression"
						},
						"FileSystem": {
							"value": "@pipeline().parameters.FileSystem",
							"type": "Expression"
						},
						"Directory": {
							"value": "@pipeline().parameters.Directory",
							"type": "Expression"
						},
						"File": {
							"value": "@pipeline().parameters.File",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"BaseUrl": {
				"type": "string",
				"defaultValue": "https://api.github.com/orgs/dotnet/repos"
			},
			"RelativeUrl": {
				"type": "string"
			},
			"FileSystem": {
				"type": "string",
				"defaultValue": "lake"
			},
			"Directory": {
				"type": "string",
				"defaultValue": "raw/github/repositories"
			},
			"File": {
				"type": "string",
				"defaultValue": "repositories"
			}
		},
		"folder": {
			"name": "Marketing"
		},
		"annotations": []
	}
}